{
  "name": "atlassian-connect-firebase",
  "version": "1.0.2",
  "description": "Atlassian Connect Firebase Adapter",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "tslint -p .",
    "prepublish": "tsc"
  },
  "lint-staged": {
    "*.{ts,md}": [
      "yarn prettier --write",
      "git add"
    ],
    "src/**/*.ts": [
      "npm run lint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/antonversal/atlassian-connect-firebase"
  },
  "keywords": [
    "uniq",
    "is uniq",
    "class-validator",
    "typeorm"
  ],
  "author": "JOIN team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/antonversal/atlassian-connect-firebase/issues"
  },
  "homepage": "https://github.com/antonversal/atlassian-connect-firebase#readme",
  "devDependencies": {
    "@firebase/testing": "^0.7.3",
    "@types/jest": "^24.0.6",
    "@types/node": "^11.9.5",
    "firebase-admin": "^7.0.0",
    "firebase-functions-test": "^0.1.6",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-extended": "^0.11.1",
    "jest-watch-typeahead": "^0.2.1",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333"
  },
  "dependencies": {}
}
